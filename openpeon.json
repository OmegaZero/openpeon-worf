{
  "cesp_version": "1.0",
  "name": "st_worf",
  "display_name": "Worf (Star Trek)",
  "version": "1.0.0",
  "author": { "name": "OmegaZero", "github": "OmegaZero" },
  "license": "MIT",
  "language": "en",
  "categories": {
    "session.start": {
      "sounds": [
        { "file": "sounds/WorfSonOfMogh.wav", "label": "I am Worf, Son of Mogh" }        
      ]
    },
    "task.acknowledge": {
      "sounds": [
        { "file": "sounds/IAmPrepared.mp3", "label": "I am prepared" }
      ]
    },
    "task.complete": {
      "sounds": [
        { "file": "sounds/AGoodDayToDie.mp3", "label": "Today is a good day to die" }
      ]
    },
    "task.error": {
      "sounds": [
        { "file": "sounds/UnidentifiedVessel.mp3", "label": "Unidentified Vessel" },
        { "file": "sounds/CannotLocateTheCaptain.mp3", "label": "Cannot locate the Captain" }
      ]
    },
    "input.required": {
      "sounds": [
        { "file": "sounds/CaptainIncomingMessage.mp3", "label": "Captain, Incoming Message" },
        { "file": "sounds/WeAreBeingHailed.wav", "label": "We are being hailed sir" },
        { "file": "sounds/HailingFrequenciesOpenSir.mp3", "label": "Hailing Frequencies Open Sir" }        
      ]
    },
    "resource.limit": {
      "sounds": [
        { "file": "sounds/WeakHuman.mp3", "label": "Blocked" },
        { "file": "sounds/MerryWorf.mp3", "label": "Sir, I protest I am not a merry man!" }
      ]
    },
    "user.spam": {
      "sounds": [
        { "file": "sounds/DoNotHugMe.mp3", "label": "Do Not Hug Me" },
        { "file": "sounds/Delay.wav", "label": "It's a tactical delay" }
      ]
    }
  }
}